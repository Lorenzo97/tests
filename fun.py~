def sum2(a,b):
    '''sums 2 numbers'''
    return a+b

def power(a,b):
    '''a^b'''
    c = a**b
    return c

def fwrite(file, L):
    channel=open(file, 'w')
    for i in L:
        channel.write(i+ '\n')
    channel.close()
    return 0
    '''
    fwrite('intro',['hi, ','myname is Lorenzo ','and I am ','a farmer '])
    '''
